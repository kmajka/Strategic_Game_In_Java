<?xml version="1.0"?>
<main>
	<!-- Main Panels -->
	<div figure="Dialog" figure_type="editor_menu" tag="MenuDialog" height="50%" transparent="Y" width="25%" temporary_displaying="Y" visible="Y" 
		main_image="tlo_info.png" inside_direction="DOWN" main_background_color="RGB(0,0,0)" >
	 	<table figure="FramePanel" tag="menu_dialog" margin_left="20%" margin_top="15%" margin_right="20%" margin_bottom="15%" temporaryDisplayed="Y"
			number_columns="1" number_rows="5" group="dialog_user_menu" visible="Y" >
		 	<cell figure="Button" figure_type="editor_menu" tag="NEW_USER_MAP_BUTTON" text="New Map" temporary_displaying="Y"
				position_column="0" position_row="0" border="Y" margin="10px" tag_default_figure="default_2" />
      <cell figure="Button" figure_type="editor_menu" tag="LOAD_LIST_OF_USER_MAPS_BUTTON" text="Load Maps" temporary_displaying="Y"
				position_column="0" position_row="1"   margin="10px" tag_default_figure="default_2" />
			<cell figure="Button" figure_type="editor_menu" tag="SAVE_USER_MAP_BUTTON" text="Save Map" temporary_displaying="Y"
				position_column="0" position_row="2"  margin="10px" tag_default_figure="default_2" />
			<cell figure="Button" figure_type="editor_menu" tag="EXIT_USER_MAP_BUTTON" text="Exit" temporary_displaying="Y"
				position_column="0" position_row="3"   margin="10px" tag_default_figure="default_2" />
			<cell figure="Button" figure_type="editor_menu" tag="RETURN_TO_EDITOR_BUTTON" text="Cancel" temporary_displaying="Y"
				position_column="0" position_row="4"  margin="10px" tag_default_figure="default_2" />
		</table>
	</div>
</main>